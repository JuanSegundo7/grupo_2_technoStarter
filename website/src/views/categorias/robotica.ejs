                        <% proyectos.forEach(proyecto => { %>
                        <h2 class="titulo-reco margin-articleInterior"><%= proyecto.nombre %></h2><% } %>
                        <% proyectos.forEach(proyecto => { %>
                        <h2 class="precio margin-articleInterior">$<%= proyecto.contribucionActual %></h2><% } %>
                        <% proyectos.forEach(proyecto => { %>
                        <p class="meta margin-articleInterior">contribuido de meta $<%= proyecto.contribucionFinal %></p><% } %>
                        <% proyectos.forEach(proyecto => { %>
                        <p class="patrocinadores margin-articleInterior"><%= proyecto.patrocinadores %></p><% } %>
                        <% proyectos.forEach(proyecto => { %>
                        <p class="patrocinadores margin-articleInterior"><%= proyecto.fecha %></p><% } %>
                        <% proyectos.forEach(proyecto => { %>
                        <p class="ubicacion margin-articleInterior"><i class="fas fa-map-marker-alt"></i><%= proyecto.ubicacion %></p><% } %>
