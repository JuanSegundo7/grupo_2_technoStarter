        <section class="titulo">
            <% switch (category.id) {  %>
                <% case 1 : %>
                    <%= category.nombre %>
                <% break %>
                <% case 2 : %>
                    <%= category.nombre %>
                <% break %>
                <% case 3 : %>
                    <%= category.nombre %>
                <% break %> 
